{
  "manifest_version": 2,

  "name": "HireBlast",
  "description": "This extension will autofill Greenhouse application forms.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}